<div class="text-center">
  <ul class="pagination">
    <% if(!isFirstPage){ %>
      <li class="prev">
        <a title="上一页" href="?p=<%= page-1 %>">上一页</a>
      </li>
    <% } %>
    <li class="active">
      <a href="javascript:void(0);"><%= page %></a>
    </li>
    <% if(!isLastPage){ %>
      <li class="next">
        <a title="下一页" href="?p=<%= page+1 %>">下一页</a>
      </li>
    <% } %>
  </ul>
</div>
